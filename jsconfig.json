{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@controllers/*": ["src/controllers/*"],
      "@middlewares/*": ["src/middlewares/*"],
      "@models": ["src/models/index.js"],
      "@modules/*": ["src/modules/*"],
      "@repositories/*": ["src/repositories/*"],
      "@utils/*": ["src/utils/*"],
      "@testHelper": ["__tests__/__testHelper__/index.js"]
    }
  },
  "exclude": [
    "dist",
    "node_modules",
    "build",
    "coverage"
  ]
}
